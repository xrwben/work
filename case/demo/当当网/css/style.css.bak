/* 轮播图开始 */
.main_bar{width:796px;float:left;
	border-bottom:1px solid #e6e6e6;
	border-right:1px solid #eaecee;overflow:hidden;}
.carousel{position:relative;}
.big_carousel{width:100%;height:296px;
	border-bottom:1px solid #e6e6e6;}
.big_carousel ul.big_pic{height:100%;}
.big_carousel ul.big_pic li{position:absolute;left:0;top:0;}
.pre_next{width:33px;height:70px;
	background:#000 url("../images/body_elements.png")no-repeat;
	opacity:0.2;position:absolute;top:50%;margin-top:-35px;
	-moz-transition:all 0.3s linear;cursor:pointer;}
.button_left{background-position:0 -70px;left:-40px;}
.button_right{background-position:-35px -70px;right:-40px;}
.big_carousel ul.button_list{width:208px;
	position:absolute;left:50%;bottom:10px;margin-left:-104px;}
.big_carousel ul.button_list li{float:left;width:20px;height:20px;
	line-height:20px;background:#646464;border-radius:50%;
	text-align:center;color:#fff;margin-right:6px;cursor:pointer;}
.big_carousel ul.button_list li.on{background:#ff2832;}
.pre_next:hover{opacity:0.5;}
.big_carousel:hover .button_left{left:0px;}
.big_carousel:hover .button_right{right:0px;}

.small_carousel{height:160px;}
.small_carousel ul{position:absolute;}
.small_carousel ul li{float:left;width:198px;height:160px;
	border-right:1px solid #e6e6e6;overflow:hidden;
	position:relative;}
.small_carousel ul li a{display:block;}
.small_carousel ul li a img{width:198px;height:160px;}
.small_carousel ul li span{width:100%;height:100%;
	background:rgba(0,0,0,0.04);display:block;position:absolute;
	left:0;top:0px;}
.small_carousel:hover .button_left{left:0px;}
.small_carousel:hover .button_right{right:0px;}
.small_carousel ul li:hover span{opacity:0;}
/* 轮播图结束 */

/* 轮播图右侧导航开始 */
.right_sidebar{width:202px;float:left;
	border-bottom:1px solid #e6e6e6;border-right:1px solid #e6e6e6;
	overflow:hidden;}
.sidebar_one{height:194px;border-top:1px solid #e6e6e6;}
.sidebar_two ul.side_title{border-top:1px solid #e6e6e6;width:204px;}
.sidebar_two ul.side_title li{float:left;border:1px solid #e6e6e6;
	width:101px;height:28px;margin-left:-1px;border-top:0;
	font-size:14px;line-height:28px;color:#646464;text-align:center;
	background:#f0f0f0;}
.sidebar_two ul.side_title li a{color:#646464;}
.sidebar_two ul.side_title li.hover{background:#fff;
	border-bottom:none;}
.info_notice{height:112px;
	border-bottom:1px solid #e6e6e6;position:relative;}
.info_notice .tab{padding-top:6px;overflow:hidden;position:absolute;
	left:0;top:0;background:#fff;}
.info_notice .tab ul li{width:183px;line-height:25px;
	padding-left:10px;
	background:url("../images/body_elements.png")no-repeat 10px -188px;}
.info_notice .tab ul li a{color:#646464;display:block;
	padding-left:10px;}
.info_notice .tab ul li:hover a{color:#ff2832;}

.sidebar_three ul.side_title{border:0;}
.sidebar_three ul.side_title li{width:67px;}
.tabs{height:90px;position:relative;}
.tabs .tab{position:absolute;left:0;top:0;}
/* 轮播图右侧导航栏结束  */

/* 畅品今日闪购开始 */
.floor_today{width:1200px;height:492px;
	margin:0 auto 30px;}
.con_left{width:996px;float:left;}
.con_title{height:34px;line-height:34px;}
