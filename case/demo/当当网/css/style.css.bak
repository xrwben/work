*{margin:0;padding:0;}
html,body{font-size:12px;font-family:arial,simsun;}
a{color:#646464;text-decoration:none;}
ul{list-style:none;}
img{display:block;}
.clearboth:after{
	display: block;
    content: "";
    height: 0;
    visibility: hidden;
    clear: both;
    font-size: 0;
    line-height: 0;
}
/*头部开始*/
.header{min-width:1200px;font-size:12px;color:#646464;}
.header_one{width:100%;height:34px;background:#f9f9f9;
	border-bottom:1px solid #f2f2f2;line-height:34px;}
.hd_one{width:1200px;height:100%;margin:0 auto;}
.toWhere{float:left;position:relative;border:1px solid #f9f9f9;}
.toWhere:hover{border:1px solid #e6e6e6;background:#fff;}
.toWhere:hover .whereHover{display:block;}
.toWhere:hover ul.area_list{display:block;left:-1px;}
.toWhere:hover a.to{background-position:right -320px;}
.whereHover{width:100%;height:1px;background:#fff;position:absolute;
	left:0;bottom:-1px;z-index:6;display:none;}
.header_one a{color:#646464;overflow:hidden;}
.header_one a.to{line-height:22px;display:block;
	padding:4px 22px 0 9px;
	background:url("../images/header_elements.png")no-repeat right -303px;}
.toWhere ul.area_list{width:298px;border:1px solid #e6e6e6;
	position:absolute;left:0px;top:26px;background:#fff;
	padding:10px 0 10px 10px;z-index:5;display:none;}
.toWhere ul.area_list li{width:48px;height:26px;float:left;
	line-height:26px;padding:0 0 0 10px;}
.toWhere ul.area_list li a:hover{color:#ff2832;}

.login_reg{float:right;line-height:24px;padding:5px 0 0 10px;}
.login_reg .login{color:#ff2832;}
.login_reg .reg:hover{color:#ff2832;}

.hd_one ul.nav{float:right;padding-top:2px;}
.hd_one ul.nav li{float:left;height:22px;line-height:21px;
	padding-top:4px;
	background:url("../images/header_elements.png")no-repeat left -285px;
	position:relative;}
.hd_one ul.nav li.car{background-image:none;}
.hd_one ul.nav li a{display:block;line-height:21px;
	padding:0 7px 0 8px;}
.hd_one ul.nav li a:hover{color:#ff2832;}
.hd_one ul.nav li.car a i{width:15px;height:14px;
	display:block;float:left;margin:3px 9px 0 0 ;
	background:url("../images/hd_cart.png")no-repeat -40px -86px;}
.hd_one ul.nav li a b{color:#ff2832;}

.hd_one ul.nav li.phone{width:84px;border:1px solid #f9f9f9;
	 /* padding-top:3px; */}
.hd_one ul.nav li.phone a i{width:11px;height:16px;
	display:block;float:left;margin:3px 9px 0 0 ;
	background:url("../images/header_elements.png")no-repeat -40px -70px;}
.phoneHover{width:100%;height:5px;background:#fff;position:absolute;
	left:0;bottom:-3px;z-index:6;display:none;}
.hd_one ul.nav li.phone:hover{
	width:84px;border:1px solid #e6e6e6;
	box-shadow:1px 1px 0px rgba(51,51,51,0.3);
	background:#fff;left:1px;}
.hd_one ul.nav li.phone:hover i{background-position:-51px -70px;}
.hd_one ul.nav li.phone:hover .phoneHover{display:block;}
.phone_erweima{width:203px;border:1px solid #efefef;
	box-shadow:1px 1px 2px rgba(51,51,51,0.3);position:absolute;left:-1px;
	top:27px;padding-left:8px;
	background:#fff url("../images/line_erweima.png")no-repeat center 29px;
	z-index:5;display:none;}
.hd_one ul.nav li.phone:hover .phone_erweima{display:block;}
.ewm{float:left;width:98px;}
.hd_one .ewm .ewm_a{text-align:center;display:block;padding:8px 0;}
.ewm a:hover span{color:#646464;}
.ewm a span{display:block;line-height:18px;}
.ewm a span.text{padding-top:8px;color:#969696;}
.ewm a img{margin:6px auto 0;}

.hd_one ul.nav li.my_dd{border:1px solid #f9f9f9;}
.hd_one ul.nav li.my_dd a.mydd_a{padding-right:27px;
	background:url("../images/header_elements.png")no-repeat right -307px;
	position:relative;}

.hd_one ul.nav li.my_dd ul.mydd_list{position:absolute;left:-1px;
	top:21px;width:83px;border:1px solid #e6e6e6;border-top:0;
	padding:4px 0;background:#fff;display:none;z-index:5;}
.hd_one ul.nav li.my_dd ul.mydd_list li{width:82px;
	height:22px;line-height:22px;padding:0;background:none;}

.hd_one ul.nav li.my_dd ul.mydd_list li:hover{background:#f2f2f2}
.hd_one ul.nav li.my_dd:hover{border:1px solid #e6e6e6;
	background:#fff;left:2px;}
.hd_one ul.nav li.my_dd:hover ul.mydd_list{display:block;}
.hd_one ul.nav li.my_dd:hover .mydd_a{background-position:right -324px;}

.new_york{float:right;padding-left:11px;padding-top:3px;
	background:url("../images/header_elements.png")no-repeat left -285px;}
.new_york a{display:block;width:85px;line-height:26px;
	padding-left:27px;color:#ff2832;
	background:url("../images/newyork_icon.png")no-repeat left 1px;}


.header_two{width:1200px;height:100px;margin:0 auto;
	position:relative;font-family:"微软雅黑";}
.ddw_logo{width:362px;position:absolute;left:0;top:0;
	vertical-align:middle;overflow:hidden;}

.search{height:48px;margin-top:20px;position:absolute;left:362px;
	top:0px;font-family:"微软雅黑";}
.search_txt{width:398px;height:18px;border:2px solid #ff2832;
	padding:9px 0 9px 10px;color:#aaa;border-right:none;
	outline:none;}
.search_text{width:92px;height:36px;border:2px solid #ff2832;
	position:absolute;left:410px;top:0px;padding-left:14px;
	border-left:0;color:#969696;background:#f6f6f6;
	line-height:36px;cursor:pointer;}
.search_text span{font-size:16px;}
