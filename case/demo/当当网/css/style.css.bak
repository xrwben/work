	text-decoration:underline;}

.search_bottom{width:585px;line-height:20px;position:absolute;
	left:362px;top:65px;}
.search_bottom ul.search_hot{width:518px;height:20px;
	overflow:hidden;float:left;}
.search_bottom ul.search_hot li{float:left;}
.search_bottom a{color:#969696;margin-right:15px;}
.search_bottom a:hover{color:#ff2832;text-decoration:underline;}
.heightter_search{float:right;}

.cart_order{height:36px;border:2px solid #dcdcdc;font-size:14px;
	position:absolute;top:20px;line-height:36px;}
.dd_cart{right:88px;width:107px;border-color:#ff2832;
	background:#ff2832 url("../images/icon_cart.png")no-repeat 5px 0;}
.dd_cart a{color:#fff;padding-left:35px;width:72px;}
.dd_cart a b{padding-left:3px;font-weight:bold;}
.dd_cart:hover{color:#ff2832;border-color:#dcdcdc;
	background:#f6f6f6 url("../images/icon_cart.png")no-repeat 5px -37px;}
.dd_cart:hover a{color:#ff2832;}
.dd_order{right:0px;width:86px;text-align:center;background:#f6f6f6;
	border-left:none;}
.dd_order a{color:#646464;display:block;}
.dd_order:hover a{color:#ff2832;}

.header_three{height:40px;border-bottom:3px solid #ff2832;
	font-family:"微软雅黑";}
.header_three ul{width:1200px;margin:0 auto;
	line-height:40px;}
.header_three ul li{float:left;text-align:center;position:relative;}
.header_three ul li.all{width:200px;margin-right:17px;
	background:#ff2832;}
.header_three ul li.all a{width:171px;padding-left:20px;display:block;
	text-align:left;color:#fff;
	background:url("../images/header_elements.png")no-repeat 161px -567px;}
.header_three ul li a{color:#323232;font-size:14px;padding:0 15px;
	display:block;font-weight:bold;}
.header_three ul li i{position:absolute;bottom:26px;left:10%;}
.header_three ul li a:hover{color:#ff2832;}
.header_three ul li.all a:hover{color:#fff;}
/*头部结束*/

/* 导航栏开始 */
.navMain{width:1200px;height:478px;margin:0 auto;
	font-family:"微软雅黑";padding-bottom:20px;}
.left_sidebar{width:200px;height:448px;padding:5px 0;
