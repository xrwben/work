	}
/*友情链接*/
#Link{
	width:100%;
	height:33px;
	border-top:1px solid #E1E1E1;
	background:#eee;
	text-align:center;
	color:#3E4146;
	line-height:33px;
	}
#Link a{
	text-decoration:none;
	color:#3E4146;
	margin:0 2px;
	}
#Link a:hover{color:#62C189;}

/*底部链接和版权部分*/
#Footer{
	width:100%;
	height:266px;
	border-top:1px solid #E1E1E1;
	background:#F7F7F7;
	}
.foot_con{
	width:1200px;
	height:74px;
	margin:0 auto;
	padding:60px 0;
	}
.foot_con ul li{
	list-style:none;
	height:74px;
	border-right:1px solid #E1E1E1;
	color:#8B8C8F;
	float:left;
	}
.foot_con ul li.one{
	width:348px;
	}
.foot_con ul li.one i{
	width:34px;
	height:34px;
	background:url("../images/headfoot_icon.png") 0px -123px;
	margin:20px 0 0 20px;
	display:block;
	float:left;
	}
.foot_con ul li.one .one_list{
	width:270px;
	padding-top:24px;
