/*右边漂浮导航栏*/
#Right_float{
	width:46px;
	position:fixed;
	right:70px;
	bottom:20px;
	}
#Right_float ul li{
	list-style:none;
	margin-bottom:12px;
	}
#Right_float ul li a{
	display:block;
	width:46px;
	height:46px;
	background:#C5C6C9;
	line-height:42px;
	text-align:center;
	border-radius:50%;
	transition:all 0.3s ease 0s;
	}
#Right_float ul li a.a_one{background:#6EB88C;}
#Right_float ul li i{
	display:inline-block;
	width:24px;
	height:24px;
	vertical-align:middle;
	}
#Right_float ul li i.i_one{
	background:url("../images/main_icon.png") -160px -368px;
	}
#Right_float ul li.li_two i.i_two{
	background:url("../images/main_icon.png") -28px -54px;
	position:relative;
	}
#Right_float ul li.li_two .wx_erweima{
	width:135px;
	height:121px;
	position:absolute;
	left:-135px;
	top:25px;
	z-index:10px;
	display:none;
	}
#Right_float ul li i.i_three{
	background:url("../images/main_icon.png") 2px -53px;
	}
#Right_float ul li i.i_four{
	background:url("../images/main_icon.png") -81px -56px;
	}
/*鼠标滑动事件*/
#Right_float ul li:hover a.a_two{background:#0c3;}
#Right_float ul li:hover .wx_erweima{display:block;}
#Right_float ul li:hover a.a_three{background:#6cf;}
#Right_float ul li:hover a.a_four{background:#099;}