	padding-left:34px;
	}
.foot_con ul li.four i{
	width:34px;
	height:34px;
	background:url("../images/headfoot_icon.png") -111px -123px;
	margin-top:20px;
	display:block;
	float:left;
	}
.foot_con ul li.four p{
	padding-left:60px;
	line-height:24px;
	}
.foot_con ul li.four:hover i{background-position:-111px -159px;}
.foot_con ul li.four:hover p{color:#3E4146;}

.copyright{
	width:100%;
	height:70px;
