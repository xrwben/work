*{margin:0;padding:0;}
body{background:#E8E8EA;font-size:14px;font-family:"微软雅黑";color:#3E4146;}
#Header{
	width:100%;
	height:54px;
	background:#2F333B;
	box-shadow:0 2px 1px rgba(0,0,0,0.25);
	}
#Header .logo{
	margin:10px 0 0 30px;
	float:left;
	}
#Header .logo a{
	width:150px;
	height:35px;
	display:inline-block;
	background:url("../images/logohover.png") no-repeat;
	}
#Header .login_register{
	float:right;
	padding-right:60px;
	}
#Header .login_register ul.loginre li{
	list-style:none;
	color:#B8BBBF;
	float:left;
	margin-right:18px;
	line-height:54px;
	}
#Header .login_register ul.loginre li a{
	text-decoration:none;
	color:#B8BBBF;
	transition:all 0.3s ease 0s;
	}
#Header .login_register ul.loginre li a:hover{color:#fff;}

#Nav{
	width:100%;
	height:45px;
	padding:15px 0;
	background:transparent -moz-linear-gradient(center top , #FFF, #EDEDF0) repeat scroll 0% 0%;
	box-shadow:0 2px 2px #DBDBDE;
	}
.nav_center{
	width:1200px;
	height:100%;
	margin:auto;
	}
.nav_con{
	width:449px;
	height:100%;
	}
.nav_con ul li{
	list-style:none;
	width:132px;
	height:44px;
	float:left;
	line-height:44px;
	}
.nav_con ul li a{
	text-decoration:none;
	color:#8B8C8F;
	font-size:18px;
	}
.nav_con ul li a i{
	display:inline-block;
	width:26px;
	height:26px;
	margin-right:5px;
	vertical-align:middle;
	}
.nav_con ul li a i.shouye{
	background:url('../images/headfoot_icon.png');
	background-position:0px -34px;
	}
.nav_con ul li a i.vip{
	background:url('../images/headfoot_icon.png');
	background-position:-29px -34px;
	}
.nav_con ul li a i.mianfei{
	background:url('../images/headfoot_icon.png');
	background-position:-58px -34px;
	}
/*首页 VIP课程 免费课 鼠标滑上去效果*/
.nav_con ul li:hover a{color:#009933;}
.nav_con ul li:hover i.shouye{background-position:0 -62px;}
.nav_con ul li:hover i.vip{background-position:-29px -62px;}
.nav_con ul li:hover i.mianfei{background-position:-58px -62px;}

/*全部课程开始*/
#All{
	width:1200px;
	height:351px;
	margin:30px auto;
	}
/*左边全部课程*/
.all_lesson{
	width:308px;
	height:351px;
	border:1px solid #E1E1E1;
	background:#fff;
	border-radius:5px;
	float:left;
	}
.lesson_title{
	height:40px;
	background:transparent -moz-linear-gradient(center top , #FFF, #F2F2F2);
	border-bottom:1px solid #E1E1E1;
	border-radius:5px 5px 0 0;
	box-shadow:0 2px 2px #F4F4F4;
	font-size:18px;
	color:#8B8C8F;
	line-height:40px;
	padding-left:20px;
	}
.all_lesson ul li{
	list-style:none;
	height:77px;
	border-bottom:1px solid #E1E1E1;
	position:relative;
	}
.all_lesson ul li .title_current{
		height:75px;
		padding-left:20px;
		}
.all_lesson ul li .title_current .it_icon{
	width:38px;
	height:38px;
	background:url("../images/main_icon.png");
	background-position:0px 0px;
	margin-top:18px;
	float:left;
	}
.all_lesson ul li .title_current .it_con{
	width:215px;
	height:66px;
	float:left;
	padding:10px 0 0 15px;
	}
.all_lesson ul li .title_current .it_con h3{
	font-size:18px;
	height:20px;
	}
.all_lesson ul li .title_current .it_con a{
	text-decoration:none;
	color:#8B8C8F;
	line-height:28px;
	}
.all_lesson ul li .title_current .it_con span{
	font-size:12px;
	padding:0 12px;
	color:#E1E1E1;
	}
.all_lesson ul li .title_current .arrow_icon{
	width:8px;
	height:13px;
	background:url("../images/main_icon.png") -245px -31px;
	float:left;
	margin-top:31px;
	}
.all_lesson ul li .title_con{
	width:260px;
	height:311px;
	border:1px solid #D0D0D0;
	background:rgba(255,255,255,0.99);
	position:absolute;
	left:308px;
	top:-1px;
	display:none;
	z-index:5;
	}
.all_lesson ul li .bor_top{
		width:100%;
		height:1px;
		background:#d0d0d0;
		position:absolute;
		top:-1px;
		left:0;
		display:none;
		}
.all_lesson	ul li .bor_bottom{
		width:100%;
		height:1px;
		background:#d0d0d0;
		position:absolute;
		bottom:-1px;
		left:0;
		display:none;
		}
.all_lesson	ul li .bor_right{
		width:1px;
		height:100%;
		background:#fff;
		position:absolute;
		top:0px;
		right:-1px;
		z-index:10;
		display:none;
		}

/*鼠标滑上去二级菜单栏显示效果*/
.all_lesson	ul li:hover .bor_top{display:block;}
.all_lesson	ul li:hover .bor_bottom{display:block;}
.all_lesson	ul li:hover .bor_right{display:block;}

.all_lesson ul li .title_con .con_up{
	width:200px;
	height:18px;
	border-bottom:1px solid #E1E1E1;
	padding:30px 0 12px 0;
	margin:0 auto;
	}
.all_lesson ul li .title_con .con_up i{
	display:block;
	width:4px;
	height:18px;
	float:left;
	}
.all_lesson ul li .title_con .con_up span{
	font-size:18px;
	padding-left:15px;
	float:left;
	line-height:18px;
	}
.all_lesson ul li .title_con .con_down{
	width:200px;
	margin:12px auto 0;
	}
.all_lesson ul li .title_con .con_down a{
	text-decoration:none;
	color:#8B8C8F;
	line-height:30px;
	display:inline-block;
	width:65px;
	height:30px;
	float:left;
	}
/*鼠标滑上去事件*/
.all_lesson ul li .title_current .it_con a:hover{color:#0c3;} 
.all_lesson ul li .title_con .con_down a:hover{color:#0c3;}
.all_lesson ul li:hover .title_con{display:block;}
.all_lesson ul li:hover .arrow_icon{display:none;}
/*右边轮播图开始*/
.all_banner{
	width:874px;
	height:351px;
	float:right;
	position:relative;
	}
.all_banner ul li{
	list-style:none;
	width:874px;
	height:351px;
	position:absolute;
	left:0;
	top:0;
	}
.all_banner ul li a img{
	width:874px;
	height:351px;
	border-radius:5px;
	display:block;
	}
.all_banner ol.banner_button{
	position:absolute;
	left:43%;
	bottom:20px;
	width:125px;
	height:20px;
	}
.all_banner ol.banner_button li{
	list-style:none;
	width:16px;
	height:16px;
	border:2px solid rgba(225,225,225,0.7);
	float:left;
	margin-right:5px;
	}
.all_banner ol.banner_button li.hover{background:#fff;}

/*精选课程、师资力量、学院风采 开始*/
#Main_Content1{
	width:1200px;
	margin:0 auto;
	}
#Main_Content2{
	width:1200px;
	margin:0 auto;
	}
#Main_Content3{
	width:1200px;
	margin:0 auto;
	}
#Main_Content4{
	width:1200px;
	margin:0 auto;
	}
.content_title{
	width:100%;
	height:28px;
	padding-top:60px;
	line-height:28px;
	}
.content_title .big_name{
	width:130px;
	font-size:24px;
	float:left;
	color:#000;
	}
.content_title ul.content_nav li{
	list-style:none;
	float:left;
	width:72px;
	height:28px;
	margin-right:10px;
	text-align:center;
	cursor:pointer;
	}
.content_title ul.content_nav li.current{
	background:#6EB88C;
	color:#fff;
	border-radius:4px;
	}
.content_title a{
	text-decoration:none;
	color:#8B8C8F;
	display:block;
	float:right;
	display:none;
	}
.content_title a.more{display:block;}
.content_title a:hover{color:#6EB88C;}

/*内容盒子*/
.content_box{
	width:1200px;
	height:247px;
	overflow:hidden;
	position:relative;
	}
/*精选课程*/
.con_list{
	width:100%;
	height:247px;
	}
.con_list ul li{
	list-style:none;
	width:260px;
	height:225px;
	border-radius:5px;
	float:left;
	margin-top:22px;
	margin-right:53px;
	box-shadow:0 1px 2px #DCDCDC;
	background:#fff;
	position:relative;
	}
.con_list ul li .li_pic{
	width:100%;
	height:178px;
	}
.con_list ul li .li_pic img{border-radius:5px;}
.con_list ul li .li_shadow{
	width:100%;
	height:26px;
	background:rgba(0,0,0,0.6);
	position:absolute;
	left:0px;
	top:152px;
	line-height:26px;
	color:#fff;
	font-size:12px;
	transition:opacity 0.3s linear 0s;
	}
.con_list ul li .li_shadow span.shadow_l{
	padding-left:10px;
	}
.con_list ul li .li_shadow span.shadow_r{
	width:67px;
	height:26px;
	background:url("images/tc_bg.png") no-repeat;
	float:right;
	text-align:center;
	}
.con_list ul li h3.kc{
	width:165px;
	height:46px;
	padding-left:10px;
	font-size:14px;
	line-height:46px;
	font-weight:normal;
	float:left;
	}
.con_list ul li h3.kc a{
	text-decoration:none;
	color:#3E4146;
	}
.con_list ul li p{
	width:72px;
	padding-left:10px;
	float:left;
	padding-top:8px;
	font-size:14px;
	line-height:18px;
	color:#62C189;
	}
.con_list ul li p span.original_price{
	font-size:12px;
	color:#8B8C8F;
	padding-left:5px;
	text-decoration:line-through;
	}
.con_list ul li .upper_shadow{
	width:260px;
	height:178px;
	background:rgba(110,184,140,0.9);
	border-radius:5px 5px 0 0;
	transition:opacity 0.3s linear 0s;
	position:absolute;
	left:0;
	top:0;
	display:none;
	}
.con_list ul li .upper_shadow .upper_info_kc{
	padding-top:50px;
	text-align:center;
	}
.con_list ul li .upper_shadow .upper_info_kc span{
	color:#fff;
	height:30px;
	line-height:30px;
	}
.con_list ul li .upper_shadow .upper_info_kc a{
	display:block;
	width:110px;
	height:40px;
	border:1px solid #fff;
	border-radius:5px;
	line-height:40px;
	text-decoration:none;
	color:#fff;
	margin:0 auto;
	}
/*课程鼠标滑上去效果*/
.con_list ul li h3.kc a:hover{color:#62C189;}
.con_list ul li:hover .upper_shadow{display:block;}
.con_list ul li .upper_shadow .upper_info_kc a:hover{
	background:#fff;
	color:#62C189;
	}
/*师资力量*/
.con_list ul li h3.sz{
	width:100%;
