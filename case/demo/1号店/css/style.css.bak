*{margin:0;
	padding:0;
	font-size:12px;
	font-family:"微软雅黑",Arial,Helvetica,sans-serif;
	color:#333;
	}
li{list-style:none;}
a{text-decoration:none;}
img{display:block;}
/*头部开始*/
#Top_header{
	width:100%;
	}
/*头部第一部分*/
.header_one{
	height:35px;
	background:#FAFAFA;
	border-bottom:1px solid #DBDBDB;
	line-height:35px;
	}
.header_hd_one{
	width:1200px;
	margin:0 auto;
	}
/*头部第一部分登录注册*/
.hd_one_left{
	float:left;
	}
.Login_register{
	position:relative;
	float:left;
	}
.loginandregister{
	height:37px;
	border-color:#F0F0F0;
	padding:0 7px 0 6px;
	border-width:0 1px 1px;
	border-style:solid;
	background:#fff;
	z-index:5;
	cursor:pointer;
	line-height:35px;
	margin-bottom:-3px;
	}
.hide{
	width:107px;
	border:1px solid #fff;
	position:relative;
	z-index:1;
	}
.loginandregister a.blue_link{color:#06C;}
/*登录中心*/
.login_center{
	width:300px;
	border:1px solid #E6E6E6;;
	position:absolute;
	left:0px;
	top:35px;
	display:none;
	padding:12px 0px 0px;
	background:#fff;
	overflow:hidden;
	z-index:1;
	}
.center_up_pic{
	margin:0px 10px;
	text-align:center;
	line-height:26px;
	float:left;
	}
.center_up_pic a.yhd_logo{
	display:block;
	width:65px;
	height:65px;
	background:url("../images/index.png") no-repeat -78px -153px;
	border:1px solid #D9D9D9;
	border-radius:3px;
	box-shadow:0 0 3px #F2F2F2;
	overflow:hidden;
	}
.center_up_text{
	padding-top:5px;
	float:left;
	line-height:24px;
	}
.center_up_text a.txt{
	height:26px;
	border:1px solid #FFB4B4;
	display:block;
	padding:0 10px;
	color:#FF8486;
	line-height:26px;
	border-radius:26px;
	margin-bottom:10px;
	overflow:hidden;
	}
.center_up_text p{
	padding-left:11px;
	clear:both;
	}
.center_up_text p a.welcome{
	padding-right:13px;
	background:url("../images/index.png") no-repeat right -151px;
	}
.center_down{
	margin-top:10px;
	height:55px;
	border-top:1px solid #E9E0DB;
	box-shadow:0px 2px 0px #F7EEE9 inset;
	line-height:55px;
	}
.center_down a.d_l{
	width:150px;
	height:53px;
	display:block;
	padding-top:0px;
	border-top:2px solid #F7EEE9;
	line-height:53px;
	background:#FFF5F0 url("../images/index.png")no-repeat right -90px;
	float:left;
	text-align:center;
	}
.center_down a.d_r{
	width:150px;
	height:53px;
	line-height:53px;
	display:block;
	border-top:2px solid #F7EEE9;
	background:#FFF5F0 url("../images/index.png")no-repeat right -90px;
	float:left;
	text-align:center;
	}
.center_arrow{
	width:52px;
	height:8px;
	background:url("../images/index.png")no-repeat 0px -870px;
	position:absolute;
	left:50%;
	bottom:-6px;
	margin-left:-26px;
	overflow:hidden;
	z-index:2;
	}
/*鼠标滑上去事件*/
.Login_register a.blue_link:hover{color:red;}
.Login_register:hover .login_center{display:block;}
.center_up_text a.txt:hover{color:#E60012;border:1px solid #E60012;}
.center_up_text p a.welcome:hover{color:#c00;}
.center_down a.d_l:hover{background:#FFEBE2;}
.center_down a.d_r:hover{background:#FFEBE2;}
.Login_register:hover .center_arrow{background-position:-60px -870px;}
.Login_register:hover{border-bottom:0px;}

/*头部第一部分省份*/
.to_where{
	height:21px;
	float:left;
	line-height:21px;
	background:url("../images/index.png") no-repeat right -710px;
	margin-top:8px;
	padding-right:39px;
	cursor:pointer;
	position:relative;
	}
.to_where span{padding:0 5px;float:left;}
.to_where a.province{
	float:left;
	padding:0px 15px 0px 5px;
	border:1px solid #DBDBDB;
	background:#fff url("../images/index.png")no-repeat right 7px;
	font-weight:bold;
	text-align:center;
	line-height:18px;
	}
.to_where .Province_hide{
	width:44px;
	height:2px;
	background:#fff;
	position:absolute;
	left:47px;
	top:19px;
	z-index:10;
	display:none;
	}
/*送达城市*/
.city{
	width:358px;
	height:300px;
	border:1px solid #ccc;
	background:#fff;
	box-shadow:0px 2px 0px rgba(234, 234, 234, 0.7);
	position:absolute;
	left:46px;
	top:20px;
	overflow:hidden;
	display:none;
	z-index:5;
	}
.city .city_hot dl.hot{
	height:30px;
	border-bottom:1px solid #E5E5E5;
	padding:6px 0 0;
	margin:0 12px;
	line-height:24px;
	}
.city .city_hot dl.hot dt{
	float:left;
	font-size:14px;
	color:#666;
	font-weight:bold;
	background:#fff;
	padding:0 10px 6px;
	position:relative;
	}
.city .city_hot dl.hot dt i{
	display:block;
	background:url("../images/index.png")no-repeat left -650px;
	width:10px;
	height:12px;
	position:absolute;
	left:-2px;
	top:-2px;
	}
.city .city_hot dl.hot dd{
	float:left;
	height:24px;
	padding-bottom:6px;
	}
.city .city_hot dl.hot dd a{
	display:block;
	float:left;
	padding:0 10px;
	color:#666;
	line-height:24px;
	}
.city .city_con{
	padding:10px 12px 0px;
	}
.city .city_con dl.city_list{
	margin-bottom:12px;
	float:left;
	}
.city .city_con dl.city_list dt{
	height:16px;
	margin:4px 0px;
	padding:0 8px 0 0;
	border-right:1px dotted #EBEBEB;
	color:#999;
	line-height:16px;
	float:left;
	}
.city .city_con dl.city_list dd{float:left;}
.city .city_con dl.city_list dd a{
	display:block;
	height:24px;
	padding:0 10px;
	color:#666;
	line-height:24px;
	}
.to_where:hover a.province{background-position:right -78px;}
.to_where:hover .city{display:block;}
.to_where:hover .Province_hide{display:block;}
.city .city_hot dl.hot dd a:hover{background:#FF966E;color:#fff;}
.city .city_con dl.city_list dd a:hover{background:#FF966E;color:#fff;}


/*头部第一部分右边*/
.hd_one_right{
	height:21px;
	float:right;
	padding-top:8px;
	line-height:21px;
	}
.hd_one_right ul.ul_left{
	float:left;
	color:#666;
	}
.hd_one_right ul.ul_left li.ul_li{
	float:left;
	height:23px;
	padding:0 6px;
	border:1px solid #FAFAFA;
	position:relative;
	z-index:1;
	}
.hd_one_right ul.ul_left li.ul_li a.li_a i.logo_mobile{
	display:inline-block;
	width:12px;
	height:16px;
	margin-right:5px;
	background:url("../images/index.png") right -230px;
	vertical-align:middle;
	}
.hd_one_right ul.ul_left li.ul_li a.li_a{
	display:block;
	float:left;
	height:14px;
	line-height:14px;
	padding:4px 18px 4px 0px;
	background:url("../images/index.png") right -255px;
	}
.hd_one_right ul.ul_left li.ul_li .list{
	width:100%;
	line-height:23px;
	border:1px solid #ccc;
	padding-top:21px;
	position:absolute;
	right:0px;
	top:0px;
	z-index:-1;
	display:none;
	}
.hd_one_right ul.ul_left li.ul_li .list ul li a{padding-left:7px;}
.hd_one_right ul.ul_left li.ul_li:hover .list{
	display:block;
	background:#fff;
	}
.hd_one_right ul.ul_left li.ul_li:hover a.li_a{background-position:right -284px;}
.hd_one_right ul.ul_left li.ul_li a:hover{color:#E60012;}

.hd_one_right span.gz{
	float:left;
	margin-left:6px;
	color:#61646E;
	}
.hd_one_right a.sina{
	display:block;
	float:left;
	width:17px;
	height:21px;
	background:url("../images/index.png")no-repeat -283px -838px;
	}
.hd_one_right a.weixin{
	display:block;
	float:left;
	width:17px;
	height:25px;
	margin-left:8px;
	background:url("../images/index.png")no-repeat -283px -928px;
	position:relative;
	}
.hd_one_right a.weixin:hover .erweima{display:block;}
.hd_one_right a.weixin .erweima{
	width:100px;
	position:absolute;
	right:0;
	top:24px;
	padding:5px 16px 16px;
	border:1px solid #ccc;
	background:#fff;
	text-align:center;
	display:none;
	z-index:2;
	}
.hd_one_right a.weixin .erweima i{
	position:absolute;
	top:-4px;
	right:5px;
	width:6px;
	height:4px;
	display:block;
	background:url("../images/index.png")no-repeat -294px -1060px;
	}
.hd_one_right a.weixin .erweima p{
	max-height:36px;
	margin-bottom:5px;
	line-height:18px;
	color:#666;
	}
/*头部第二部分开始*/
.header_two{
	width:1200px;
	margin:0 auto 5px;
	position:relative;
	}
.hd_two_smallbanner img{display:block;}
.hd_two_bigbanner img{
	display:block;
	display:none;
	}
.pic_fold{
	width:17px;
	height:38px;
	display:block;
	background:#666;
	color:#fff;
	position:absolute;
	right:21px;
	top:0px;
	text-align:center;
	padding-top:2px;
	cursor:pointer;
	}
.pic_fold s{
	display:block;
	width:0px;
	height:0px;
	border:5px solid #fff;
	border-left:5px solid transparent;
	border-right:5px solid transparent;
	border-bottom:5px solid transparent;
	margin-top:-1px;
	margin-left:3px;
	}
/*头部第三部分开始*/
.header_three{
	width:1200px;
	margin:0 auto;
	padding:15px 0 12px;
	position:static;
	}
/*头部第三部分左边logo*/
.hd_three_left{
	width:204px;
	float:left;
	}
.three_l_logo img{float:left;}
.three_l_logo img{float:left;}
/*头部第三部分中间搜索*/
.hd_three_center{
	float:left;
	margin-left:114px;
	}
.search{
	height:32px;
	position:relative;
	}
.search_list{
	width:68px;
	height:28px;
	background:#F8F8F8;
	position:absolute;
	left:2px;
	top:2px;
	border:1px solid #E1E1E1;
	border-width:0px 1px;
	overflow:hidden;
	font-size:14px;
	}
.search_list a{
	width:53px;
	line-height:28px;
	display:block;
	padding-left:15px;
	}
.search_list i{
	width:7px;
	height:10px;
	display:block;
	background:url("../images/index.png")no-repeat -235px -60px;
	position:absolute;
	top:12px;
	right:12px;
	-moz-transition:all 0.3s ease 0s;
	}
.search_list:hover i{background-position:-235px -70px;}
.search_list a:hover{color:#C00;}
.search_list:hover{height:auto;border-bottom:1px solid #E1E1E1;}

.search_content{border:2px solid #FF3C3C;}
.search_txt{
	width:342px;
	padding-left:78px;
	padding-right:6px;
	height:28px;
	border:0;
	float:left;
	background:#fff;
	}
.txt_list{
	width:455px;
	height:280px;
	border:1px solid #e1e1e1;
	background:#fff;
	position:absolute;
	top:32px;
	right:0px;
	display:none;
	}
.search_button{
	width:98px;
	height:28px;
	border:none;
	background:#FF3C3C;
	font-size:14px;
	float:left;
	color:#fff;
	text-align:center;
	cursor:pointer;
	line-height:28px;
	}
.three_center_list{height:24px;line-height:24px;}
.three_center_list a{color:#999;margin-right:10px;}
.three_center_list a:hover{color:#E60012;}
/*头部第三部分右边福利与订单查询*/
.hd_three_right{
	float:right;
	padding-right:82px;
	position:relative;
	}
.three_right_fl{
	width:60px;
	height:39px;
	display:block;
	border:1px solid #F2F2F2;
	padding-top:4px;
	background:#FCFCFC;
	float:left;
	text-align:center;
	position:relative;
	}
.three_right_fl em{
	width:22px;
	height:15px;
	display:inline-block;
	background:url("../images/index.png")no-repeat -130px -20px;
	}
.three_right_fl p{
	color:#999;
	line-height:16px;
	}
.three_right_fl i{
	width:5px;
	height:3px;
	display:block;
	background:url("../images/index.png")no-repeat -130px -40px;
	margin:0 auto;
	}
.three_right_ddcx{
	width:60px;
	height:39px;
	display:block;
	border:1px solid #F2F2F2;
	padding-top:4px;
	background:#FCFCFC;
	float:left;
	text-align:center;
	position:relative;
	}
.three_right_ddcx em{
	width:22px;
	height:15px;
	display:inline-block;
	background:url("../images/index.png")no-repeat -130px -20px;
	}
.three_right_ddcx p{
	color:#999;
	line-height:16px;
	}
.three_right_ddcx i{
	width:5px;
	height:3px;
	display:block;
	background:url("../images/index.png")no-repeat -130px -40px;
	margin:0 auto;
	}
.three_right_shopping{
	width:83px;
	height:49px;
	background:#FF3C3C;
	position:absolute;
	right:0;
	bottom:0;
	}
.shopping_arrow{
	width:4px;
	height:4px;
	position:absolute;
	left:-4px;
	top:0px;
	background:url("../images/index.png")no-repeat -150px -40px;
	}
.shoping_car{
	height:16px;
	display:block;
	line-height:16px;
	font-size:14px;
	text-align:center;
	color:#fff;
	padding:15px 0 16px;
	}
.shoping_car em{
	width:19px;
	height:17px;
	display:inline-block;
	margin-right:5px;
	background:url("../images/index.png")no-repeat -190px -20px;
	vertical-align:middle;
	}
.shoping_login{
	width:358px;
	height:267px;
	border-color:#FF3C3C #DCDCDC #DCDCDC;
	border-width:3px 1px 1px;
	border-style:solid;
	background:#fff;
	position:absolute;right:0px;top:48px;
	display:none;
	z-index:2;
	}
.three_right_shopping:hover .shoping_login{display:block;}

/*头部第四部分商品导航开始*/
.header_four{
	height:34px;
	border-bottom:2px solid #FD3F49;
	}
.hd_four_nav{
	width:1200px;
	height:34px;
	margin:0 auto;
	}
.nav_left{float:left;position:relative;}
.allList{
	width:168px;
	height:34px;
	display:block;
	background:#FF3C3C url("../images/index.png")no-repeat -104px -1203px;
	font-size:14px;
	color:#fff;
	font-weight:bold;
	line-height:34px;
	padding-left:42px;
	
	}
.nav_left_allContent{
	width:210px;
	background:#C23131;
	border-top:1px solid #A01111;
	position:relative;
	margin-top:2px;
	position:absolute;
	left:0;
	top:34px;
	z-index:1;
	}
.nav_left_allContent ul{
	border-top:2px solid #B01D1D;
	}
.nav_left_allContent ul li{
	margin-bottom:1px;
	}
.curr_nav{
	padding-left:5px;
	background:#872222;
	-moz-transition-delay:0.2s;
	-moz-transition:all 0.2s linear;
	}
.nav_left_allContent ul li h3.list_h3{
	height:32px;
	padding-right:10px;
	color:#fff;
	background:url("../images/index.png")no-repeat 190px -1148px;
	line-height:32px;
	}
/*使用阿里图标*/
@font-face {font-family: 'iconfont';
	src: url('../iconfont/iconfont.eot'); /* IE9*/
	src: url('../iconfont/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
	url('../iconfont/iconfont.woff') format('woff'), /* chrome、firefox */
	url('../iconfont/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
	url('../iconfont/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}
.iconfont{
	font-family:"iconfont" !important;
	font-size:18px;font-style:normal;
	color:#fff;
	-webkit-font-smoothing: antialiased;
	-webkit-text-stroke-width: 0.2px;
	-moz-osx-font-smoothing: grayscale;
	}
.nav_left_allContent ul li h3.list_h3 i{
	width:28px;
	display:inline-block;
	margin-right:10px;
	text-align:right;
	vertical-align:middle;
	}
.nav_left_allContent ul li h3.list_h3 a{
	color:#fff;
	font-weight:normal;
	}
.nav_left_allContent ul li h3.list_h3 a:hover{
	text-decoration:underline;
	}

.list_sorts{
	width:990px;
	height:398px;
	background:#cc66ff;
	position:absolute;
	left:210px;
	top:-1px;
	display:none;
	z-index:10;
	}

.nav_center{
	width:778px;
	height:34px;
	float:left;
	}
.nav_center ul li{
	line-height:34px;
	float:left;
	}
.nav_center ul li a{
	height:100%;
	display:block;
	font-size:14px;
	font-weight:bold;
	padding:0 20px;
	}
.nav_center ul li a.light{color:#FF3C3C;}
.nav_center ul li a:hover{color:#FF3C3C;}

.nav_right{
	width:205px;height:40px;float:right;
	margin-top:-6px;
	}

/*广告轮播开始*/
#banner{
	width:100%;
	height:398px;
	min-width:1200px;
	}
.banner_ol_pic{
	width:100%;
	height:398px;
	position:relative;
	}
.banner_ol_pic ol li{
	width:100%;
	height:398px;
	background:#ccccff;
	position:absolute;
	left:0;
	top:0;
	z-index:-1;
	display:none;
	}
.banner_ol_pic ol li:nth-child(1){background:#FAE103;display:block;}
.banner_ol_pic ol li:nth-child(2){background:#930C12;}
.banner_ol_pic ol li:nth-child(3){background:#FFAA0F;}
.banner_ol_pic ol li:nth-child(4){background:#5ADEE1;}
.banner_ol_pic ol li:nth-child(5){background:#EAD0FF;}
.banner_ol_pic ol li:nth-child(6){background:#F5F6F1;}
.banner_ol_pic ol li:nth-child(7){background:#B3BED0;}
.banner_ol_pic ol li:nth-child(8){background:#72D3E3;}
.banner_ol_pic ol li a{
	width:1200px;
	height:100%;
	margin:0 auto;
	display:block;
	}
.banner_ol_pic ol li a img{margin-left:210px;}

.banner_button{
	width:45px;
	height:80px;
	position:absolute;
	left:50%;
	top:150px;
	display:none;
	}
.show_pre{
	background:rgba(0,0,0,0.4) url("../images/index2.png")no-repeat 0px -220px;
	margin-left:-390px;
	}
.show_next{
	background:rgba(0,0,0,0.4) url("../images/index2.png")no-repeat -80px -220px;
	margin-left:315px;
	}
.banner_ol_pic:hover .banner_button{display:block;}

.banner_ol_pic ul.banner_smallbtn{
	position:absolute;
	left:42%;
	bottom:20px;
	}
.banner_ol_pic ul.banner_smallbtn li{
	width:15px;
	height:15px;
	background:#ccc;
	float:left;
	margin-right:10px;
	}
.banner_ol_pic ul.banner_smallbtn li.banner_curr_hover{background:#FF3C3C;}

/*轮播图右边的文字广告*/
.banner_right_text{
	width:240px;
	height:398px;
	background:#fff;
	position:absolute;
	left:50%;
	top:1px;
	margin-left:360px;
	}
.banner_text_top{height:93px;}
.banner_text_top dl{
	float:left;
	text-align:center;
	padding:12px 9px;
	}
.banner_text_top dl dd{padding:0 6px;}
.banner_text_top dl dd a{display:block;}
.banner_text_top dl dd a:hover{color:#E60012;}
.banner_text_top dl dd a img{display:block;margin:0 auto;}

.banner_text_center{
	height:66px;
	padding:8px 15px;
	border-top:1px solid #eee;
	}
.banner_text_center ul.banner_txt_content li{
	width:214px;
	height:22px;
	line-height:22px;
	}
.banner_text_center ul.banner_txt_content li a{color:#666;}
