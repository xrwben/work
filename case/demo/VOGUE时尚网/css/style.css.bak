*{margin:0;padding:0;}
body{font-size:14px;font-family:"微软雅黑";}
img{display:block;}
a{text-decoration:none;color:#000;}
li{list-style:none;}
em,i{font-style:normal;}
.clearboth:after{
	content:"";
	clear:both;
	height:0px;
	font-size:0;
	display:block;
	visibility:hidden;
	}

/*头部开始*/
#header{width:100%;height:90px;}
/*头部第一部分*/
.hd_up{height:36px;background:#000;line-height:36px;}
.wrapper{width:1000px;margin:0 auto;position:relative;}
.header_icon{width:23px;height:18px;display:block;
	background:url("../images/header_icon.png")no-repeat;margin-top:10px;
	float:left;}
.icon_sina{background-position:-139px 0px;margin-left:9px;}
.icon_weixin{background-position:-162px 0px;margin:10px 18px 0;position:relative;}
.erweima{display:block;position:absolute;left:0;top:25px;display:none;z-index:1;}
.header_text{height:16px;display:block;color:#AFAFAF;line-height:16px;
	border-left:1px solid #929292;padding:0 14px;float:left;margin-top:10px;}
.otherColor{color:#BB9A6C;}
.header_text span{font-family:"Times New Roman";font-style:italic;padding:0 2px;}

.header_search{height:28px;float:left;margin:4px 0 0 5px;
	background:#353535;}
.search_text{width:170px;height:28px;display:block;float:left;margin-left:5px;
	border:0;background:#353535;color:#fff;transition:all 0.5s;}
.search_text:focus{width:260px;}
.header_search em{height:19px;border-left:1px solid #898989;margin-top:5px;
	float:left;display:block;}
.search_btn{width:19px;height:19px;float:left;margin:5px;border:0;
	background:url("../images/header_icon.png")no-repeat -185px 0;cursor:pointer;}
.login_register{float:left;color:#AFAFAF;padding:0 10px;
	position:absolute;cursor:pointer;}
.login{right:60px;}
.register{right:10px;}
/*头部第一部分*/

/*头部导航部分*/
.hd_down{width:100%;height:51px;border-width:1px 0px 2px;border-style:solid;border-color:#E1E1E1;}
.fixed_nav{position:fixed;top:0;z-index:10;background:#fff;}
.header_nav{width:1000px;margin:0 auto;font-size:16px;position:relative;}
.header_nav ul li{float:left;margin-top:18px;border-right:1px solid #C1C3C3;
	height:16px;line-height:16px;}
.header_nav ul li a{color:#000;padding:0px 13px 0px 12px;}

.nav_content{width:185px;position:absolute;top:35px;display:none;z-index:1;}
.nav_c1{left:57px;}
.nav_c2{left:114px;}
.nav_c3{left:171px;}
.nav_c4{left:228px;}
.nav_c5{left:285px;}
.nav_c6{left:361px;}
.nav_c7{left:437px;}
.nav_c8{left:494px;}
.item_head{height:16px;background:url("../images/nav-item-header.png")no-repeat;}
.item_body{padding-top:16px;background:url("../images/nav-item-body.png")repeat-y;}
.item_bottom{height:7px;background:url("../images/nav-item-footer.png")no-repeat;
	margin-top:-1px;}
.nav_list{width:165px;height:31px;margin:0 auto;line-height:31px;font-size:14px;
	text-indent:10px;border-bottom:1px dashed #ABABAB;position:relative;}
.nav_list:last-child{border:0;}
.nav_list a{color:#545454;}
.nav_list span{position:absolute;right:30px;opacity:0;}
.nav_list:hover a{color:#CE0000;}
.nav_list:hover span{color:#CE0000;right:10px;opacity:1;-moz-transition:all 1s;}

.hd_nav_sub{float:left;margin-top:7px;height:35px;line-height:35px;
	border-right:1px solid #C1C3C3;}
.hd_nav_sub a{color:#000;padding:0 16px;}
.hd_nav_sub a.fx{color:#CE0000;}
.hd_nav_sub a.VT{padding-left:0;}
.hd_nav_right{float:left;font-size:12px;width:90px;line-height:24px;}
.hd_nav_right a{color:#000;padding-left:15px;}
/*头部导航部分*/
/*头部结束*/

/*LOGO 部分*/
#big_logo{height:178px;background:#fff;overflow:hidden;}
#big_logo img{width:485px;height:109px;margin:35px auto 0;}

/*banner广告轮播开始*/
.banner{border-top:1px solid #D8D8D8;margin-bottom:30px;overflow:hidden;}
.banner_con{width:1000px;height:564px;margin:0 auto;
			position:relative;}
.banner_con ul.banner_con_ul{width:1200%;position:absolute;
	left:-2000px;top:0;}
.banner_con ul.banner_con_ul li{float:left;position:relative;}
.pic_title{width:100%;height:95px;text-align:center;
	background:url("../images/slide-mask.png")no-repeat center top;
	color:#fff;
	position:absolute;left:0px;bottom:40px;}
.pic_title a{font-size:30px;color:#fff;line-height:62px;
	text-shadow:0px 1px 3px #000;}
.pic_title p{margin:0 auto;overflow:hidden;
	white-space:nowrap;text-overflow:ellipsis;}

.banner_con ul.banner_button{position:absolute;left:40%;bottom:20px;}
.banner_con ul.banner_button li{width:9px;height:9px;background:#bbb;
	border-radius:50%;float:left;margin:0 8px;cursor:pointer;}
.banner_con ul.banner_button li.hover_smallBtn{background:#CE0000;}

.banner_shadow{width:1000px;height:564px;background:#000;opacity:0.5;
	position:absolute;top:0px;}
.banner_shadow_l{right:1000px;}
.banner_shadow_r{left:1000px;}

.banner_arrow{width:57px;height:73px;cursor:pointer;
	background:url("../images/element.png")no-repeat 0px -223px;
	position:absolute;top:50%;margin-top:-36px;opacity:0;transition:all 1s;}
.arrow_pre{left:0px;}
.arrow_next{right:0px;}
.banner_con:hover .banner_arrow{opacity:1;}

.banner_arrow i{width:57px;height:73px;display:block;
	background:url("../images/element.png")no-repeat;}
.arrow_pre i{background-position:0 -77px;}
.arrow_next i{background-position:0 -150px;}

.bottomLine{margin:0 auto;}

/*今日推荐开始*/
.content{min-width:1000px;}
.what_new{margin-bottom:50px;}
.content_hd{margin-bottom:35px;position:relative;}
.hd_line{width:100%;height:45px;border-bottom:1px solid #D8D8D8;
	position:absolute;left:0;}
.hd_wrapper{width:1000px;margin:0 auto;position:relative;}
._logo{width:320px;height:87px;margin:0 auto;
	background:url("../images/title-common.jpg")no-repeat -10px 0;}
.hd_text{width:100%;height:24px;position:absolute;
	left:0;top:17px;}
.txt{width:340px;height:20px;position:absolute;}
.top_left{left:0;top:0;text-align:right;}
.top_right{right:0;top:0;text-align:left;}
.top_left a{font-size:16px;color:#555;}
.marginRight{margin-right:15px;}
.txt a:hover{color:#E00024;}

.content_bd{width:1000px;margin:0 auto;position:relative;}
.c_pic{width:270px;border:1px solid #D8D8D8;padding:12px 16px 0;float:left;
	margin-right:44px;transition:all 0.3s;}
.c_pic a{display:block;color:#000;}
.c_pic a span{display:block;line-height:56px;font-size:16px;text-overflow:ellipsis;
	white-space:nowrap;}
.c_pic:hover{box-shadow:0 0 9px #666;}
.c_pic a:hover{color:#E00024;}

.c_list{width:270px;border:1px solid #D8D8D8;float:right;padding:12px 16px 13px;}
.c_list ul{padding-top:13px;}
.c_list ul li{line-height:16px;margin-bottom:14px;padding-left:32px;}
.c_list ul li:nth-child(1){background:url("../images/num.png")no-repeat;}
.c_list ul li:nth-child(2){background:url("../images/num.png")no-repeat 0 -30px;}
.c_list ul li:nth-child(3){background:url("../images/num.png")no-repeat 0 -60px;}
.c_list ul li:nth-child(4){background:url("../images/num.png")no-repeat 0 -90px;}
.c_list ul li:nth-child(5){background:url("../images/num.png")no-repeat 0 -120px;}
.c_list ul li:nth-child(6){background:url("../images/num.png")no-repeat 0 -150px;}
.c_list ul li:nth-child(7){background:url("../images/num.png")no-repeat 0 -180px;}
.c_list ul li:nth-child(8){background:url("../images/num.png")no-repeat 0 -210px;}
.c_list ul li:nth-child(9){background:url("../images/num.png")no-repeat 0 -240px;}
.c_list ul li:nth-child(10){background:url("../images/num.png")no-repeat 0 -270px;}
.c_list ul li a{color:#000;text-overflow:ellipsis;white-space:nowrap;display:block;
	vertical-align:middle;}
.c_list ul li a:hover{color:#E00024;}

